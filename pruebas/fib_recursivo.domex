program FibonacciRecursivo;
var
int : num;

function int fibonacci(int : x) {
    if (x == 0) {
        return 0;
    }
    if (x == 1 || x == 2) {
        return 1;
    }
    return fibonacci(x - 1) + fibonacci(x - 2);
}

main() {
    print("Until what position of the fibonacci sequence would you like");
    input(num);
    print("fibonacci number of pos", num);
    print(fibonacci(num));
}