program FactorialIterativo;
var
int : i, num;

function int factorial(int : x) {
    var int : f;
    f = 1;
    if(x <= 0) {
        return 0;
    }
    i = 1;
    while (i <= x) {
        f = f * i;
        i = i + 1;
    }
    return f;
}

main() {
    print("Write number to get factorial");
    input(num);
    print(factorial(num));
}